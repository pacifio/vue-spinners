{
  "name": "vue-spinners",
  "description": "A vue js component library which contains multiple spinners from SpinKit",
  "version": "1.0.2",
  "author": {
    "name": "pacifio",
    "email": "adibmohsin.root@gmail.com"
  },
  "keywords": [
    "vue",
    "vuejs",
    "plugin",
    "spinner",
    "components-library",
    "vue-plugin"
  ],
  "license": "MIT",
  "main": "dist/vue-spinners.common.js",
  "unpkg": "dist/vue-spinners.browser.js",
  "module": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config config/webpack.config.dev.js --progress --watch",
    "build": "npm run build:browser && npm run build:common",
    "build:browser": "cross-env NODE_ENV=production webpack --config config/webpack.config.browser.js --progress --hide-modules",
    "build:common": "cross-env NODE_ENV=production webpack --config config/webpack.config.common.js --progress --hide-modules",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pacifio/vue-spinners.git"
  },
  "bugs": {
    "url": "https://github.com/pacifio/vue-spinners/issues"
  },
  "homepage": "https://github.com/pacifio/vue-spinners#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^3.2.4",
    "css-loader": "^0.26.4",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^2.3.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "uglify-js": "^3.3.27",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vue-loader": "^10.3.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^2.7.0",
    "webpack-merge": "^1.1.2"
  }
}
